  "license": "MIT",
  "bugs": {
    "url": "https://github.com/onubo/react-native-logs/issues"
  },
  "homepage": "https://github.com/onubo/react-native-logs#readme",
  "devDependencies": {
    "@types/node": "^14.14.34",
    "jest": "^26.6.3",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "dateformat": "^4.5.1"
  }
